import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        int count=0, times=0, increment=0;
        boolean go=true;
        Scanner input=new Scanner(System.in);
        
        int num=input.nextInt();
        
        while(go){
            if(times*6+1>=num){
                go=false;
            }
            count++;
            increment++;
            times+=increment;
        }
        
        System.out.println(count);
    }
}
